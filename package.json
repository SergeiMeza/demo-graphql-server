{
  "name": "demo-server",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/SergeiMeza/demo-server",
  "author": "Sergei Meza",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "dev": "ts-node src/index.ts",
    "build": "rimraf ./dist && tsc && copyfiles -u 1 src/**/*.graphql dist && copyfiles -u 1 src/**/*.json dist",
    "generate:ts": "graphqlgen",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-server-express": "latest",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "graphql": "latest",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "nanoid": "^2.0.1",
    "nanoid-dictionary": "^2.0.0",
    "saslprep": "^1.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@graphql-codegen/typescript-resolvers": "^1.9.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.135",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^1.2.1",
    "@types/response-time": "^2.3.3",
    "@types/uuid": "^3.4.4",
    "typescript": "^3.4.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 80
  }
}
